# Environment variables for the application
# Copy this file to .env and set your own values

# Metrics authentication (required for production)
METRICS_USER=admin
METRICS_PASS=your_secure_password_here

# Application settings
FLASK_ENV=production
FLASK_DEBUG=false

# Gunicorn settings
WORKERS=3
TIMEOUT=30
KEEP_ALIVE=2

# Prometheus settings
PROMETHEUS_PORT=8080 